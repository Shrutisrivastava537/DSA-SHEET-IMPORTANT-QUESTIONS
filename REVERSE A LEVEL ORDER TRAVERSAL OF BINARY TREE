#include<iostream>
#include<bits/stdc++.h>
using namespace std;

struct Node
{
    Node *lchild;
    int data;
    Node *rchild;
}*root=NULL;

// Creating a Binary Tree

void treecreate()
{
    Node *t,*p;
    int x;
    
    queue<Node *> q;
    cout<<"ENTER THE ROOT VALUE :";
    cin>>x;
    
    root=new Node;
    root->data=x;
    root->lchild=root->rchild=NULL;
    q.push(root);
    
    while(!q.empty())
    {
        p=q.front();
        q.pop();
        
        cout<<"ENTER LEFT CHILD 0F "<<p->data<<" : ";
        cin>>x;
        
        if(x!=-1)
        {
            t=new Node;
            t->data=x;
            t->lchild=t->rchild=NULL;
            p->lchild=t;
            q.push(t);
        }
        
        cout<<"ENTER RIGHT CHILD OF "<<p->data<<" : ";
        cin>>x;
        
        if(x!=-1)
        {
            t=new Node;
            t->data=x;
            t->lchild=t->rchild=NULL;
            p->rchild=t;
            q.push(t);
        }
    }
}


void reverselevelorder(Node *root)
{
    vector<int> ans;
    
    if(!root) return ;
    
    queue<Node *>q;
    q.push(root);
    
    while(!q.empty())
    {
        Node *t= q.front();
        ans.push_back(t->data);
        
        if(t->rchild) q.push(t->rchild);
        if(t->lchild) q.push(t->lchild);
        q.pop();
    }
    
    reverse(ans.begin(),ans.end());
    
    for(int i=0;i<ans.size();i++)  cout<<ans[i]<<" ";
    
    
}


// MAIN FUNCTION 

int main()
{
    treecreate();
    cout<<endl;
    
    cout<<"REVERSE LEVELORDER TRAVERSAL "<<endl;
    reverselevelorder(root);
    
    
    
    return 0;
}
